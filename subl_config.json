{
	"caret_extra_bottom": 0,
	"caret_extra_top": 3,
	"caret_extra_width": 1,
	"drag_text": false,
	"font_face": "Code New Roman",
	"font_options":
	[
		"gray_antialias"
	],
	"font_size": 14,
	"highlight_line": true,
	"ignored_packages":
